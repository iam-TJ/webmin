openssl_cmd=/usr/bin/openssl
ssl_dir=/usr/share/ssl
ssl_cert_dir=/usr/share/ssl/certs
ssl_key_dir=/usr/share/ssl/private
ssl_csr_dir=/usr/share/ssl/csr
ssl_cnf_file=/usr/share/ssl/openssl.cnf
